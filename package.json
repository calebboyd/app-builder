{
  "name": "app-builder",
  "version": "6.1.0",
  "description": "Simple, Composable, promise based composable functions",
  "main": "./lib/app-builder.js",
  "typings": "./lib/app-builder.d.ts",
  "author": "Caleb Boyd (https://github.com/calebboyd)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calebboyd/app-builder/issues"
  },
  "homepage": "https://github.com/calebboyd/app-builder",
  "files": [
    "lib",
    "!**/*.spec.js"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*\" --fix",
    "build": "npm run lint && tsc",
    "prepublish": "npm run test",
    "watch:typescript": "tsc -w",
    "watch:test": "jest --watchAll",
    "watch": "npm run build && concurrently -k \"npm:watch:typescript\" \"npm:watch:test\" -c blue,green",
    "test": "npm run build && jest --passWithNoTests"
  },
  "jest": {
    "testRegex": ".*?(\\.spec).js",
    "watchPathIgnorePatterns": [
      "\\.ts$"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calebboyd/app-builder.git"
  },
  "keywords": [
    "middleware",
    "functional",
    "composable",
    "composition",
    "pipeline",
    "async",
    "compose"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "concurrently": "^5.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "~6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-watch": "^6.0.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  }
}
